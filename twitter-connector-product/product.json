{
  "installers": [
    {
      "id": "maven-import",
      "data": {
        "projects": [
          {
            "groupId": "com.axonivy.connector.twitter",
            "artifactId": "twitter-connector-demo",
            "version": "${version}",
            "type": "iar"
          }
        ],
        "repositories": [
          {
            "id": "axonivy.public",
            "url": "https://maven.axonivy.com",
            "snapshots": {
              "enabled": "true"
            }
          }
        ]
      }
    },
    {
      "id": "maven-dependency",
      "data": {
        "dependencies": [
          {
            "groupId": "com.axonivy.connector.twitter",
            "artifactId": "twitter-connector",
            "version": "${version}",
            "type": "iar"
          }
        ],
        "repositories": [
          {
            "id": "axonivy.public",
            "url": "https://maven.axonivy.com",
            "snapshots": {
              "enabled": "true"
            }
          }
        ]
      }
    }
  ]
}
